<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.is.main.bankBook.BankBookDAO">
 	
 	<select id="getList" resultType="com.is.main.bankbook.BankBook.DTO">
 		SELECT BOOKNUM, BOOKNAME, BOOKRATE, BOOKSALE FROM BANKBOOK ORDER BY BOOKNUM DESC
 	</select>
 	
 	<select id="getDetail" resultType="com.is.main.bankbook.BankBook.DTO" parameterType="com.is.main.bankbook.BankBook.DTO">
 		SELECT *FROM BANKBOOK WHERE BOOKNUM=#{bookNum}
 	</select>
 	
 
  </mapper>